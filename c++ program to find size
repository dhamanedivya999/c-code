#include<iostream>
using namespace std;
int main(){
    cout<<"size of char is:"<<sizeof(char)<<"byte"<<endl;
    cout<<"size of int is:"<<sizeof(int)<<"byte"<<endl;
    cout<<"size of float is:"<<sizeof(float)<<"byte"<<endl;
    cout<<"size of double is:"<<sizeof(double)<<"byte"<<endl;
    return 0;
  }
